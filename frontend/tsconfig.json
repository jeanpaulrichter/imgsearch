{
    "compilerOptions": {
        "outDir": "../debug/www/js",
        "allowJs": false,
        "target": "es2020",
        "module": "es2020",
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "moduleResolution": "node16",
        "sourceMap": true,
        "rootDir": "src",
        "baseUrl": "src",
        "mapRoot": "/js/",
        "lib": ["DOM"]
    },
    "include": ["src/**/*"]
}